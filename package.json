{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.3.2",
  "scripts": {
    "kill-port": "PORT=4173 bash ./scripts/kill-port.sh",
    "dev": "bun run kill-port && bun run dev:frontend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "discord:bot": "bash ./scripts/run-discord-bot.sh",
    "discord:bot:dry-run": "cd services/discord-bot && DISCORD_DRY_RUN=1 bun src/index.js",
    "build": "vite build",
    "serve": "PORT=4173 bash ./scripts/kill-port.sh && vite preview --host 0.0.0.0 --port 4173",
    "deploy:convex": "convex deploy",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.90",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2",
    "@fal-ai/client": "^1.9.3",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.3.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/colorthief": "^2.6.1",
    "clsx": "^2.1.1",
    "colorthief": "^2.6.0",
    "convex": "^1.32.0",
    "git": "^0.1.5",
    "gsap": "^3.14.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.2.0",
    "lucide-react": "^0.562.0",
    "openai": "^6.25.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.2.1",
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "~10.4.24",
    "concurrently": "^9.2.1",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^15.15.0",
    "playwright": "^1.58.2",
    "postcss": "~8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.2.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^6.4.1"
  }
}
